extend layout
block content
    div#logo
        h3 Yamaha Dashboard
    div.modal.fade#loading
        div.modal-dialog
            img(src='../img/ajax-loader.gif')
    div.mainwrapper#content
        ul.nav.nav-tabs
            li.active
                a(data-toggle="tab" href='#bs_mygarageMG') My Garage: Sport Heritage
            li
                a(data-toggle="tab" href='#bs_mygarageMT') My Garage: MT
            li
                a(data-toggle="tab" href='#bs_mygarageSS') My Garage: Super Sport
        div.tab-content
            div.tab-pane.fade.in.active#bs_mygarageMG
                div.chart-element
                    h4 My Garage: Sports Heritage
                    button#export-mygarageMG Export CSV
                    #garage_mygarage
                div.chart-element
                    h4 Big Five Country AppStore Downloads
                    #appStore-coutries-mygarage
            div.tab-pane.fade#bs_mygarageMT
                div.chart-element
                    h4 My Garage: MT
                    button#export-mygarageMT Export CSV
                    #garage_mygaragemt
                div.chart-element
                    h4 Big Five Country AppStore Downloads
                    #appStore-coutries-mygaragemt
            div.tab-pane.fade#bs_mygarageSS
                div.chart-element
                    h4 My Garage: Super Sport
                    button#export-mygarageSS Export CSV
                    #garage_mygaragesupersport
                div.chart-element
                    h4 Big Five Country AppStore Downloads
                    #appStore-coutries-mygaragesupersport
        div.time-interval
            select#time-interval
                option(value='' selected='select' disabled='disabled') Time Interval
                option(value='week') Week
                option(value='month') Month
                option(value='year') Year
