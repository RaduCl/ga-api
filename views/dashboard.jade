        div.col-md-6.widget
            div.panel.panel-primary
                div.widget-header.panel-heading App Usage
                div.app-widget-body.panel-body
                    div.chart-element
                        h4 New versus Returning Users
                        div#new-vs-returning-users
                    div.chart-element
                        h4 Top 5 Parts by Total Users
                        div#top-parts
                    div.chart-element.col-sm-12.col-md-6
                        h4 Top 5 countries by Android users
                        div#top-countries-android
                    div.chart-element.col-sm-12.col-md-6
                        h4 Top 5 countries by iOS users
                        div#top-countries-iOS
                    div.chart-element
                        h4 Total Downloads by OS
                        div#total-downloads-byOS
                    div.chart-element.col-md-6.col-lg-6
                        h4 Total Downloads by Google Play Store
                        div#google-play-store-downloads
                    div.chart-element.col-md-6.col-lg-6
                        h4 Total Downloads by App Store
                        div#app-store-downloads
        div.col-md-6.widget
            div.panel.panel-primary
                div.widget-header.panel-heading Retention
                div.app-widget-body.panel-body
