doctype html
html
	head
		title="Yamaha-dashboard"
		meta(charset="utf-8")
		meta(content="width=device-width, initial-scale=1", name="viewport")
		link(href="styles/main.css", rel="stylesheet", type="text/css")
		link(href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css", rel="stylesheet", type="text/css")
		//jquery CDN
		script(src="https://code.jquery.com/jquery-2.1.4.js", type="text/javascript")
		//bootstrap JS
		script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js", type="text/javascript")
		//D3 js (data visualization)
		script(src="https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.6/d3.min.js" charset="utf-8")
		//Google Charts API
		script(src="https://www.google.com/jsapi" charset="utf-8")

	body
		div.container
			div#logo
				h3 Yamaha Dashboard
			div.mainwrapper
				div.col-md-6.widget
					div.panel.panel-primary
						div.widget-header.panel-heading App Usage
						div.app-widget-body.panel-body
							//h5.widget-subtitle New versus Returning Users
							h4 New versus Returning Users
							div#new-vs-returning-users
							h4 Top 5 Countries by Total Users
							div#top-countries
							div#total-downloads-byOS
							div.col-md-6
								h4 Total Downloads by Google Play Store
								div#google-play-store-downloads
							div.col-md-6
								h4 Total Downloads by App Store
								div#app-store-downloads

				div.col-md-12.widget
					div.panel.panel-primary
						div.widget-header.panel-heading Retention
						div.app-widget-body.panel-body
							//h5.widget-subtitle User Increase / Decrease
							div#retention
							div#user-increase

		script(src="scripts/app.js")